{
    "name": "brynj-digital/laravel-scout-vectorize",
    "description": "Cloudflare Vectorize driver for Laravel Scout",
    "keywords": [
        "laravel",
        "scout",
        "vectorize",
        "cloudflare",
        "semantic-search",
        "vector-search",
        "embeddings"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryn Jones",
            "email": "bryn@lunarvis.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/scout": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutVectorize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutVectorize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutVectorize\\VectorizeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
